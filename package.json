  },
  "homepage": "https://github.com/jrobison153/osprey#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "binary-search-insert": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "chai": "^4.1.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
